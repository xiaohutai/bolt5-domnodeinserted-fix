{
    "name": "xiaohutai/domnodeinsertedfix",
    "description": "Fix for DOMNodeInserted deprecated issue in Bolt 5",
    "type": "bolt-extension",
    "license": "MIT",
    "authors": [
        {
            "name": "Xiao-Hu Tai",
            "email": "xiao.tai@ontwerpwerk.nl"
        }
    ],
    "require": {
        "php": ">=7.1.3"
    },
    "require-dev": {
        "bolt/core": "^5.0.0",
        "symplify/easy-coding-standard": "^10.2"
    },
    "autoload": {
        "psr-4": {
            "DomNodeInsertedFix\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "entrypoint": "DomNodeInsertedFix\\Extension"
    }
}